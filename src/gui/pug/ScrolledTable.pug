mixin ScrolledTableBox(title, editButtonId, columnNames, rows)
    .ScrolledTableBox
        h3= title
        button(id= editButtonId) Edit
        table
            tr(class="ScrolledTableColumns")
                each name in columnNames
                    th= name
        .ScrollingBox
            table
                if(rows != null)
                    each row in rows
                        tr
                        if(row != null)
                            each val in row
                                td= val