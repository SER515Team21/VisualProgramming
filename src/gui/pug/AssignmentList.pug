include TabSwitcher

mixin assignmentBlock(name, dueDate)
    a() name
    p dueDate


#AssignmentList(hidden='')
    include header
    .tab-switcher
        +tabbedFrame(['Past Due', 'Upcoming'], ['past_due_tab', 'upcoming_tab'])
        .view.active(id='past_due_tab')

        .view(id='upcoming_tab')
