include GenericDashboard
include TabSwitcher
include studentSearch

#TeacherView(hidden='')
    +GenericDashboard()
        +LeftPane()
            .tab-switcher
                +tabbedFrame(['Courses', 'Students'], [courses, students])
            +student-search
        +MainPane()
            .tab-switcher
                +tabbedFrame(['Students', 'Assignments'], [courseStudents, assignments])
                .view.active(id='View Students')
                    table(id='studentList')
                        thead
                            tr
                                th Last
                                th First
                        tbody
                            tr
                                td Musk
                                td Elon
                .view(id='View Assignments')
